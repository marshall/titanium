<string id="string-allow">อนุญาต</string>
<string id="string-allow-accesskey">อ</string>
<string id="string-deny">ปฏิเสธ</string>
<string id="string-deny-accesskey">ป</string>
<string id="string-cancel">ยกเลิก</string>
<string id="string-never-allow-link">ไม่อนุญาตไซต์นี้</string>
